<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="portal_my_home_assessment" name="Portal My Home : Assessment" inherit_id="portal.portal_my_home" priority="50">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Assessment</t>
                <t t-set="url" t-value="'/assessment'"/>
                <t t-set="placeholder_count" t-value="'assessment_count'"/>
            </t>
        </xpath>
    </template>
</odoo>
